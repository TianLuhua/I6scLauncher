                <action android:name="com.boyue.boyuelauncher.main.fragments.hht_ly_fragment.hht_yspy.htcz" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".main.fragments.hht_ly_fragment.hht_yspy.syzl.HHT_yspy_yszl_Activity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="com.boyue.boyuelauncher.main.fragments.hht_ly_fragment.hht_yspy.syzl" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".main.fragments.hht_ly_fragment.hht_yspy.zqyy.HHT_yspy_zqyy_Activity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="com.boyue.boyuelauncher.main.fragments.hht_ly_fragment.hht_yspy.zqyy" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

    </application>


</manifest>