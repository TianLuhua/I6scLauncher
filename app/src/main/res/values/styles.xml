<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="fragment_autoplay_tab_center">
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:ellipsize">marquee</item>
    </style>

    <style name="NoTitleTranslucentTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/colorTransparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="TextStyle_Launcher">

    </style>

    <style name="TextStyle_Settings">


    </style>

    <style name="TextStyle_Settings_Item" parent="TextStyle_Settings">

    </style>

    <style name="TextStyle_Settings_Info" parent="TextStyle_Settings">

    </style>

    <style name="TextStyle_WifiManager">


    </style>

    <style name="Stlye_setting_item">
        <item name="android:background">@drawable/select_settings_item_check</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="Stlye_system_setting_indicator_line">
        <item name="android:layout_marginEnd">5dp</item>
        <item name="android:layout_marginStart">5dp</item>
        <item name="android:background">#b5b5b5</item>
        <item name="android:layout_width">223dp</item>
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="Stlye_system_setting_text">
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">22sp</item>
    </style>

</resources>
