<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipChildren="false">

    <LinearLayout
        android:id="@+id/hht_ly_yzyx_01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="114dp"
        android:layout_marginTop="116dp"
        android:gravity="center"
        android:orientation="vertical">

        <com.boyue.boyuelauncher.widget.EnlargeAndNarrowAnimationView
            android:id="@+id/hht_ly_yzyx_01_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/ic_hht_ly_klok_ttmtv_33" />

        <TextView
            android:id="@+id/hht_ly_yzyx_01_lable"
            style="@style/Stlye_Item_lable_01_text"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="@string/ic_hht_ly_klok_ttmtv_33" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/hht_ly_yzyx_02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="74dp"
        android:layout_marginTop="116dp"
        android:layout_toEndOf="@+id/hht_ly_yzyx_01"
        android:gravity="center"
        android:orientation="vertical">

        <com.boyue.boyuelauncher.widget.EnlargeAndNarrowAnimationView
            android:id="@+id/hht_ly_yzyx_02_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/ic_hht_ly_klok_ttmtv_34" />

        <TextView
            android:id="@+id/hht_ly_yzyx_02_lable"
            style="@style/Stlye_Item_lable_01_text"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="@string/ic_hht_ly_klok_ttmtv_34" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/hht_ly_yzyx_03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="74dp"
        android:layout_marginTop="116dp"
        android:layout_toEndOf="@+id/hht_ly_yzyx_02"
        android:gravity="center"
        android:orientation="vertical">

        <com.boyue.boyuelauncher.widget.EnlargeAndNarrowAnimationView
            android:id="@+id/hht_ly_yzyx_03_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:src="@mipmap/ic_hht_ly_klok_ttmtv_35" />

        <TextView
            android:id="@+id/hht_ly_yzyx_03_lable"
            style="@style/Stlye_Item_lable_01_text"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="@string/ic_hht_ly_klok_ttmtv_35" />
    </LinearLayout>


</RelativeLayout>
